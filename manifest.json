{
    "name": "tabmarkers",
    "version": "1.0",
    "description": "tabmarkers",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ],
    "manifest_version": 2
}
